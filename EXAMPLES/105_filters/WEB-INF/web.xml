<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0"
  metadata-complete="true">

  <display-name>Welcome to Tomcat</display-name>
  <description>
     Welcome to Tomcat
  </description>


  <filter>
	<filter-name>fl003</filter-name>
	<filter-class>filters.EFilter</filter-class>
  </filter>

  <filter-mapping>
	<filter-name>fl003</filter-name>
	<servlet-name>app001</servlet-name>
  </filter-mapping>

  <filter>
	<filter-name>fl002</filter-name>
	<filter-class>filters.BFilter</filter-class>
  </filter>

  <filter-mapping>
	<filter-name>fl002</filter-name>
	<url-pattern>/act.do</url-pattern>
  </filter-mapping>


  <servlet>
	<servlet-name>app001</servlet-name>
	<servlet-class>controllers.ActionServlet</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>app001</servlet-name>
	<url-pattern>/act.do</url-pattern>
  </servlet-mapping>

  
  <filter>
	<filter-name>fl005</filter-name>
	<filter-class>filters.DFilter</filter-class>
  </filter>

  <filter-mapping>
	<filter-name>fl005</filter-name>
	<url-pattern>/act.do</url-pattern>
  </filter-mapping>
  
  <filter>
	<filter-name>fl004</filter-name>
	<filter-class>filters.CFilter</filter-class>
  </filter>

  <filter-mapping>
	<filter-name>fl004</filter-name>
	<servlet-name>app001</servlet-name>
  </filter-mapping>

  <filter>
	<filter-name>fl001</filter-name>
	<filter-class>filters.AFilter</filter-class>
  </filter>

  <filter-mapping>
	<filter-name>fl001</filter-name>
	<url-pattern>/act.do</url-pattern>
  </filter-mapping>





</web-app>
